{
  "title": "Coolector.Api",
  "version": "1.0.0-*",
  "buildOptions": {
    "emitEntryPoint": true
  },
  "dependencies": {
    "Coolector.Common": "1.0.*",
    "Coolector.Services.Operations.Shared": "1.0.*",
    "Coolector.Services.Remarks.Shared": "1.0.*",
    "Coolector.Services.Users.Shared": "1.0.*",
    "Coolector.Services.Statistics.Shared": "1.0.*",
    "Autofac": "4.2.1",
    "Autofac.Extensions.DependencyInjection": "4.0.0",
    "Lockbox.Client": "1.0.0",
    "Exceptionless": "4.0.1922",
    "jose-jwt": "2.1.0",
    "RawRabbit": "1.10.3",
    "RawRabbit.vNext": "1.10.3",
    "Microsoft.AspNetCore.Hosting": "1.1.0",
    "Microsoft.AspNetCore.Server.Kestrel": "1.1.0",
    "Microsoft.AspNetCore.Owin": "1.1.0",
    "Microsoft.AspNetCore.Cors": "1.1.0",
    "Microsoft.Extensions.Caching.Memory": "1.1.0",
    "Microsoft.Extensions.Configuration.Binder": "1.1.0",
    "Microsoft.Extensions.Configuration.Json": "1.1.0",
    "Microsoft.AspNetCore.Authentication.JwtBearer": "1.1.0",
    "NLog.Extensions.Logging": "1.0.0-*",
    "Nancy": "2.0.0-barneyrubble",
    "Nancy.Authentication.Stateless": "2.0.0-barneyrubble",
    "System.Reflection.TypeExtensions": "4.3.0",
    "Polly": "5.0.1-v5-0-alpha0001"
  },
  "frameworks": {
	  "netcoreapp1.1": {
		  "dependencies": {
			  "Microsoft.NETCore.App": {
				  "type": "platform",
				  "version": "1.1.0"
			  }
		  }
	  }
  },
  "runtimeOptions": {
    "configProperties": {
      "System.GC.Server": true
    }
  },
  "publishOptions": {
    "include": [
      "web.config",
      "nlog.config",
      "appsettings.json"
    ]
  }
}
